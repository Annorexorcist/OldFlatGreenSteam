	downarrow
	{
      image="graphics/icon_down_default"    
	}

	downarrow:hover
	{
      image="graphics/icon_down_hover"

	
			render_bg {
			
			//background
			0="fill( x0 + 5, y0 + 4, x1 - 2, y1 - 4, buttonfaceactive )"
			//lines around
			1="fill(x0 + 4, y0 + 5, x0 + 5, y1 - 5, buttonfaceactive )"  //left
			2="fill(x1 - 2, y0 + 5, x1 - 1, y1 - 5, buttonfaceactive )"  //right
			}
	}

	uparrow
	{
      image="graphics/icon_up_default"    
	}
	
	rightarrow
	{
      image="graphics/icon_right_default"   
	}
	
	
	rightarrow:hover
	{
      image="graphics/icon_right_hover"    
	}
	
    Button
	{
		font-family = basefont
		inset = "4 0 0 0"
		textcolor = White
		render_bg {
			1="fill( x0 + 1, y0, x1 - 1, y0 + 1, ButtonBorder )"  // top
			2="fill( x0 + 1, y1 - 1, x1 - 1, y1, ButtonBorder )"  // bottom
			3="fill( x0, y0 + 1, x0 + 1, y1 - 1, ButtonBorder )"  // left
			4="fill( x1 - 1, y0 + 1, x1, y1 - 1, ButtonBorder )"  // right

			5="fill( x0 + 1, y0 + 1, x1 - 1, y1 - 1, ClayEnabled )" // inside

			6="fill( x0+1, y0+1, x0+2, y0+2, ButtonBorderCorner )"  // top-left
			7="fill( x1-2, y0+1, x1-1, y0+2, ButtonBorderCorner )"  // top-right
			8="fill( x0+1, y1-2, x0+2, y1-1, ButtonBorderCorner )"  // bottom-left
			9="fill( x1-2, y1-2, x1-1, y1-1, ButtonBorderCorner )"  // bottom-right
		}
	}

	Button:hover
	{
		textcolor = Over
	}

	Button:active
	{
		textcolor = Over
		render_bg {
			1="fill( x0 + 1, y0, x1 - 1, y0 + 1, Over )"  // top
			2="fill( x0 + 1, y1 - 1, x1 - 1, y1, Over )"  // bottom
			3="fill( x0, y0 + 1, x0 + 1, y1 - 1, Over )"  // left
			4="fill( x1 - 1, y0 + 1, x1, y1 - 1, Over )"  // right

			5="fill( x0 + 1, y0 + 1, x1 - 1, y1 - 1, ClayEnabled )" // inside

		6="fill( x0+1, y0+1, x0+2, y0+2, ButtonBorderCornerSelected )"  // top-left
		7="fill( x1-2, y0+1, x1-1, y0+2, ButtonBorderCornerSelected )"  // top-right
		8="fill( x0+1, y1-2, x0+2, y1-1, ButtonBorderCornerSelected )"  // bottom-left
		9="fill( x1-2, y1-2, x1-1, y1-1, ButtonBorderCornerSelected )"  // bottom-right
		}
	}

	Button:disabled
	{
		textcolor = ClayDisabledText
		render_bg {
			1="fill( x0 + 1, y0, x1 - 1, y0 + 1, ButtonBorderDisabled )"  // top
			2="fill( x0 + 1, y1 - 1, x1 - 1, y1, ButtonBorderDisabled )"  // bottom
			3="fill( x0, y0 + 1, x0 + 1, y1 - 1, ButtonBorderDisabled )"  // left
			4="fill( x1 - 1, y0 + 1, x1, y1 - 1, ButtonBorderDisabled )"  // right

		6="fill( x0+1, y0+1, x0+2, y0+2, ButtonBorderCornerDisabled )"  // top-left
		7="fill( x1-2, y0+1, x1-1, y0+2, ButtonBorderCornerDisabled )"  // top-right
		8="fill( x0+1, y1-2, x0+2, y1-1, ButtonBorderCornerDisabled )"  // bottom-left
		9="fill( x1-2, y1-2, x1-1, y1-1, ButtonBorderCornerDisabled )"  // bottom-right
		}
	}

 
	"Page Button" {
		render_bg {
			1="fill( x0 + 1, y0, x1 - 1, y0 + 1, ButtonBorder )"  // top
			2="fill( x0 + 1, y1 - 1, x1 - 1, y1, ButtonBorder )"  // bottom
			3="fill( x0, y0 + 1, x0 + 1, y1 - 1, ButtonBorder )"  // left
			4="fill( x1 - 1, y0 + 1, x1, y1 - 1, ButtonBorder )"  // right

			5="fill( x0 + 1, y0 + 1, x1 - 1, y1 - 1, LightClayButtonBG )" // inside

		6="fill( x0+1, y0+1, x0+2, y0+2, PageButtonBorderCorner )"  // top-left
		7="fill( x1-2, y0+1, x1-1, y0+2, PageButtonBorderCorner )"  // top-right
		8="fill( x0+1, y1-2, x0+2, y1-1, PageButtonBorderCorner )"  // bottom-left
		9="fill( x1-2, y1-2, x1-1, y1-1, PageButtonBorderCorner )"  // bottom-right
		}
	}

	"Page Button:active" {
		textcolor = Over
		render_bg {
			1="fill( x0 + 1, y0, x1 - 1, y0 + 1, Over )"  // top
			2="fill( x0 + 1, y1 - 1, x1 - 1, y1, Over )"  // bottom
			3="fill( x0, y0 + 1, x0 + 1, y1 - 1, Over )"  // left
			4="fill( x1 - 1, y0 + 1, x1, y1 - 1, Over )"  // right

			5="fill( x0 + 1, y0 + 1, x1 - 1, y1 - 1, LightClayButtonBG )" // inside

		6="fill( x0+1, y0+1, x0+2, y0+2, ButtonBorderCornerSelected )"  // top-left
		7="fill( x1-2, y0+1, x1-1, y0+2, ButtonBorderCornerSelected )"  // top-right
		8="fill( x0+1, y1-2, x0+2, y1-1, ButtonBorderCornerSelected )"  // bottom-left
		9="fill( x1-2, y1-2, x1-1, y1-1, ButtonBorderCornerSelected )"  // bottom-right
		}
	}

	"Page Button:disabled" {
		textcolor = LightGrayBG
		render_bg {
		1="fill( x0 + 1, y0, x1 - 1, y0 + 1, PageButtonBorderDisabled )"  // top
		2="fill( x0 + 1, y1 - 1, x1 - 1, y1, PageButtonBorderDisabled )"  // bottom
		3="fill( x0, y0 + 1, x0 + 1, y1 - 1, PageButtonBorderDisabled )"  // left
		4="fill( x1 - 1, y0 + 1, x1, y1 - 1, PageButtonBorderDisabled )"  // right

	6="fill( x0+1, y0+1, x0+2, y0+2, PageButtonBorderCornerDisabled )"  // top-left
	7="fill( x1-2, y0+1, x1-1, y0+2, PageButtonBorderCornerDisabled )"  // top-right
	8="fill( x0+1, y1-2, x0+2, y1-1, PageButtonBorderCornerDisabled )"  // bottom-left
	9="fill( x1-2, y1-2, x1-1, y1-1, PageButtonBorderCornerDisabled )"  // bottom-right
		}
	}
    
         
